{
    "name": "masterei/laravel-signer",
    "description": "Laravel signed URL wrapper with additional feature such as consumable and user-based signed URLs.",
    "keywords": [
        "masterei",
        "laravel-signer",
        "consumable signed url",
        "user-based signed url"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rey Arnejo Jr.",
            "email": "mastereijunior@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/url": "^2.4"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.4",
        "orchestra/testbench": "^9.0",
        "robiningelbrecht/phpunit-pretty-print": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Masterei\\Signer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masterei\\Signer\\Tests\\": "tests/",
            "Masterei\\Signer\\Tests\\Database\\Factories\\": "tests/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterei\\Signer\\SignerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
